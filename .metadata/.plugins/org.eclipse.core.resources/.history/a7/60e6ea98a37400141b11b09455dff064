package Modèle;
import java.util.*;


/**
 * 
 */
public class Troncon extends Observable {

	public enum Etat {
		Bouché,
		Libre,
		Beau;
	}
	
	/**
	 * 
	 */
	public Troncon() {
	}

	/**
	 * 
	 */
	private String nomRue;

	/**
	 * 
	 */
	private int vitesse ;

	/**
	 * 
	 */
	private int longueur ;

	/**
	 * 
	 */
	
	private Etat etatTroncon;

	/**
	 * 
	 */
	private Noeud[] fin;



	/**
	 * 
	 */
	private Noeud[] origine;


}