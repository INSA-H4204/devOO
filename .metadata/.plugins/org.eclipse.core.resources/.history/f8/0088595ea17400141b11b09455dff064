package Modèle;
import java.util.*;

/**
 * 
 */
public class Troncon extends Observable {

	/**
	 * 
	 */
	public Troncon() {
	}

	/**
	 * 
	 */
	private String nomRue;

	/**
	 * 
	 */
	private int vitesse ;

	/**
	 * 
	 */
	private int longueur ;

	/**
	 * 
	 */
	public enum etat {
		Bouché,
		Libre,
		Très beau;
	}

	/**
	 * 
	 */
	private Noeud[] fin;



	/**
	 * 
	 */
	private Noeud[] origine;


}