package Modèle;

import java.util.*;

/**
 * Un noeud peut être une Livraison ou une intersection, un carrefour.
 * 
 * @author hgerard
 */
public class Noeud extends Observable {

	private int noeudID ;
	private int posX ;
	private int posY ;
	
	/**
	 * 
	 */
	public Noeud() {
		noeudID = 0;
		posX = 0;
		posY = 0;
	}

}