
package Controleur;

/**
 * 
 */
public abstract class Commande {

	/**
	 * 
	 */
	public Commande(int idClient, int idNoeud, int idNoeudPrecedent) {
	}

	/**
	 * 
	 */
	public Commande() {
	}

	/**
	 * 
	 */
	protected abstract void undo();

	/**
	 * 
	 */
	protected abstract void redo();

	/**
	 * 
	 */
	protected abstract void execute();

}